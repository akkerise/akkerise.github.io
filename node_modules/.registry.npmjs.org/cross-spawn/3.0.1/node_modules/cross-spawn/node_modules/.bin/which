#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\which\1.3.0\node_modules\which\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\which\1.3.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\which\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\cross-spawn\3.0.1\node_modules\which\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\cross-spawn\3.0.1\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\cross-spawn\node_modules" "$basedir/node"  "$basedir/../../../which/bin/which" "$@"
  ret=$?
else 
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\which\1.3.0\node_modules\which\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\which\1.3.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\which\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\cross-spawn\3.0.1\node_modules\which\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\cross-spawn\3.0.1\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\cross-spawn\node_modules" node  "$basedir/../../../which/bin/which" "$@"
  ret=$?
fi
exit $ret
