#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node-sass\4.5.3\node_modules\node-sass\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node-sass\4.5.3\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node-sass\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\gulp-sass\3.1.0\node_modules\node-sass\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\gulp-sass\3.1.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\gulp-sass\node_modules" "$basedir/node"  "$basedir/../../../node-sass/bin/node-sass" "$@"
  ret=$?
else 
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node-sass\4.5.3\node_modules\node-sass\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node-sass\4.5.3\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node-sass\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\gulp-sass\3.1.0\node_modules\node-sass\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\gulp-sass\3.1.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\gulp-sass\node_modules" node  "$basedir/../../../node-sass/bin/node-sass" "$@"
  ret=$?
fi
exit $ret
