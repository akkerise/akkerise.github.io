#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\strip-indent\1.0.1\node_modules\strip-indent\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\strip-indent\1.0.1\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\strip-indent\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\redent\1.0.0\node_modules\strip-indent\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\redent\1.0.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\redent\node_modules" "$basedir/node"  "$basedir/../../../strip-indent/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\strip-indent\1.0.1\node_modules\strip-indent\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\strip-indent\1.0.1\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\strip-indent\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\redent\1.0.0\node_modules\strip-indent\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\redent\1.0.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\redent\node_modules" node  "$basedir/../../../strip-indent/cli.js" "$@"
  ret=$?
fi
exit $ret
