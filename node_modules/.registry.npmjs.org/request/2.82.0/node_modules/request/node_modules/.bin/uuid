#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\uuid\3.1.0\node_modules\uuid\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\uuid\3.1.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\uuid\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\request\2.82.0\node_modules\uuid\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\request\2.82.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\request\node_modules" "$basedir/node"  "$basedir/../../../uuid/bin/uuid" "$@"
  ret=$?
else 
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\uuid\3.1.0\node_modules\uuid\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\uuid\3.1.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\uuid\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\request\2.82.0\node_modules\uuid\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\request\2.82.0\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\request\node_modules" node  "$basedir/../../../uuid/bin/uuid" "$@"
  ret=$?
fi
exit $ret
