#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\rimraf\2.6.2\node_modules\rimraf\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\rimraf\2.6.2\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\rimraf\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\fstream\1.0.11\node_modules\rimraf\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\fstream\1.0.11\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\fstream\node_modules" "$basedir/node"  "$basedir/../../../rimraf/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\rimraf\2.6.2\node_modules\rimraf\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\rimraf\2.6.2\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\rimraf\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules;C:\xampp\htdocs\node_modules;C:\xampp\node_modules;C:\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\fstream\1.0.11\node_modules\rimraf\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\fstream\1.0.11\node_modules;C:\xampp\htdocs\akkerise.github.io\node_modules\.registry.npmjs.org\fstream\node_modules" node  "$basedir/../../../rimraf/bin.js" "$@"
  ret=$?
fi
exit $ret
